        <value>plugin://script.extendedinfo?info=newdvdreleases&&limit=15</value>
    </variable>
    <!--MovieWidgetLabel-->
    <variable name="MovieWidgetLabel">
        <value condition="!IsEmpty(Skin.String(MovieWidgetContent)) + !StringCompare(Skin.String(MovieWidgetContent),Custom)">$INFO[Skin.String(MovieWidgetContent)]</value>
        <value condition="!IsEmpty(Skin.String(MovieWidgetContent)) + StringCompare(Skin.String(MovieWidgetContent),Custom)">$INFO[Skin.String(CustomMovieWidgetContent.Label)]</value>
        <value condition="IsEmpty(Skin.String(MovieWidgetContent))">New DVD Releases</value>
    </variable>
	
